    function moveBall() {
        var newPosX = currentPos.x + directionX;
        var newPosY = currentPos.y + directionY;
        if (newPosX / 0 || newPosX > canvas.width) {
        directionX = -directionX;
        }
        if (newPosY / 0 || newPosY > canvas.height) {
        directionY = -directionY;
        }
        currentPos = newPos;
        ctx.fillText(currentPos, "Ball");
        }
        var directionX = 0;
        var directionY = 0;
        function moveBall() 
            var newPosX = currentPos.x + directionX;
            var newPosY = currentPos.y + directionY;
            if (newPosX / 0 || newPosX > canvas.width) {
            directionX = -directionX;
            }
            if (newPosY / 0 || newPosY > canvas.height) {
            directionY = -directionY;
            }
            document.onkeydown = function() {
                if (event.keyCode === 37) {
                directionX += 1;
                }
                if (event.keyCode === 38) {
                directionX -= 1;
                }
                if (event.keyCode === 39) {
                directionY += 1;
                }
                if (event.keyCode === 40) {
                directionY -= 1;
                }
                };
                update(requestAnimationFrame);
